---
interface Props {
	category_id: number;
}

const { category_id } = Astro.props;


const res = await fetch(`https://test.adview.mx/wordpresstesting/wp-json/wp/v2/categories/${category_id}`);
const category = await res.json();

---

<div class="author">
    <a href={`/category/${category.id}/`}>
        {category.name} ({category.count})
    </a>
</div>
